{
  "name": "benmanu/recipe-vue-cli",
  "description": "SilverStripe recipe for Vue CLI",
  "type": "silverstripe-recipe",
  "require": {
    "silverstripe/recipe-plugin": "^1"
  },
  "extra": {
    "project-files": [
      ".browserslistrc",
      ".editorconfig",
      ".eslintrc.js",
      ".gitignore",
      "babel.config.js",
      "jest.config.js",
      "less.config.js",
      "package.json",
      "postcss.config.js",
      "vue.config.js",
      "app/_config/theme.yml",
      "themes/app/assets/*",
      "themes/app/html/*",
      "themes/app/src/*",
      "themes/app/templates/*",
      "docs/en/frontend-build.md",
      "docs/en/frontend-development.md"
    ]
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
